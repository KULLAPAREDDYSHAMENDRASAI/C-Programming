#include <stdio.h>

int main() {
    int number;

    // Prompt user for input
    printf("Enter a number: ");
    scanf("%d", &number);

    // Display multiplication table
    printf("Multiplication table for %d:\n", number);
    for (int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", number, i, number * i);
    }

    return 0;
}
